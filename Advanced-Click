:randomTouchD
	if #astralAwakening == 1
		if #hero == 31
		//Miki
			touchDown 0 425 225
			touchUp 0
		elseif #hero == 32
		//Mina
			touchDown 0 465 120
			touchUp 0
		elseif #hero == 33
		//Yzafa
			touchDown 0 385 135
			touchUp 0
		elseif #hero == 34
		//Zolom
			touchDown 0 60 170
			touchUp 0
		elseif #hero == 35
		//Finn
			touchDown 0 465 125
			touchUp 0
		elseif #hero == 36
		//Nohni
			touchDown 0 120 145
			touchUp 0
		elseif #hero == 37
		//Damon
			touchDown 0 420 80
			touchUp 0
		endif
		
		sleep #hitDelay2
		#time = #time + #hitDelay
		
		if #hero > 36
			#hero = 0
		endif
	endif
	
	if #hitCount > 12
		#hitCount = 0
		goto :extra
	endif
	
	if #agree == 1 or #decline == 1 or #PHoM == 1
		#hitCount = #hitCount + 1
	endif
goto :start

:extra
	#hitCount = 0
	if #PHoM == 1
		touchDown 0 270 370
		sleep 12
		touchUp 0
		sleep 14
		touchPress 0 270 370
		sleep 2
		touchUp
		sleep 3
		touchPress 0 270 370
		sleep 16
		touchUp 0
		sleep 18
		touchDown 0 270 370
		sleep 10
		touchUp 0
		sleep 18
		#time = #time + 60
	endif

	if #agree == 1
		touchDown 0 300 630
		sleep 40
		touchUp 0
		sleep 40
		touchDown 0 300 630
		sleep 40
		touchUp 0
		sleep 40
		#time = #time + 180
	endif

	if #decline == 1
		touchDown 0 200 630
		sleep 30
		touchUp 0
		sleep 30
		touchDown 0 200 630
		sleep 30
		touchUp 0
		sleep 30
		#time = #time + 150
	endif
goto :randomTouch
