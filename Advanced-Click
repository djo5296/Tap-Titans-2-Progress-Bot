			touchUp 0
		elseif #hero == 14
		//Beany
			touchDown 0 425 305
			touchUp 0
		elseif #hero == 15
		//Sawyer
			touchDown 0 400 225
			touchUp 0
		elseif #hero == 16
		//Lil' Ursa
			touchDown 0 465 410
			touchUp 0
		elseif #hero == 17
		//Saje
			touchDown 0 20 380
			touchUp 0
		elseif #hero == 18
		//Wally
